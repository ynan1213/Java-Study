[
  {
    "extension": "revapi.filter",
    "configuration": {
      "elements": {
        "include": [
          {
            "matcher": "java-package",
            "match": "/org\\.apache\\.log4j(\\..*)?/"
          }
        ],
        "exclude": [
          "class org\\.apache\\.logging\\.log4j\\.core\\.config\\.plugins\\.util\\.PluginType"
        ]
      }
    }
  }
]
