<Configuration name="ReliabilityStrategyTest" status="OFF">
  <Appenders>
    <List name="list">
      <PatternLayout pattern="%m"/>
    </List>
    <Async name="async">
      <AppenderRef ref="list"/>
    </Async>
  </Appenders>
  <Loggers>
    <Root level="DEBUG">
      <AppenderRef ref="async"/>
    </Root>
  </Loggers>
</Configuration>
